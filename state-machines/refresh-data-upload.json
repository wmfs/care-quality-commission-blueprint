{
  "Comment": "Refresh CQC Data via CSV File",
  "name": "Refresh CQC Data via CSV File",
  "version": "1.0",
  "categories": [
    "cqc"
  ],
  "StartAt": "ImportDirectory",
  "States": {
    "ImportDirectory": {
      "Type": "Task",
      "Resource": "module:getConfiguredRegistryKey",
      "ResourceConfig": {
        "key": "cqc_cqcImportDirectory"
      },
      "Parameters": {
        "defaultValue": false
      },
      "ResultPath": "$.importDirectory",
      "Next": "Process"
    },
    "Process": {
      "Type": "Task",
      "Resource": "function:cqc_refreshDataUpload",
      "End": true
    }
  },
  "restrictions": [
    {
      "roleId": "dm_dataManager",
      "allows": [
        "*"
      ]
    }
  ]
}
